<?xml version="1.0"?>
<tryton>
    <data>
        <record model="ir.ui.view" id="contingency_keys_view_tree">
            <field name="model">account.contingency_keys</field>
            <field name="type">tree</field>
            <field name="name">contingency_keys_tree</field>
        </record>
        <record model="ir.ui.view" id="contingency_keys_view_form">
            <field name="model">account.contingency_keys</field>
            <field name="type">form</field>
            <field name="name">contingency_keys_form</field>
        </record>

        <record model="ir.action.act_window" id="act_contingency_keys_tree">
            <field name="name">Contingency Keys</field>
            <field name="res_model">account.contingency_keys</field>
        </record>
        <record model="ir.action.act_window.view" id="act_contingency_keys_tree_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="contingency_keys_view_tree"/>
            <field name="act_window" ref="act_contingency_keys_tree"/>
        </record>
        <record model="ir.action.act_window.view" id="act_contingency_keys_tree_view2">
            <field name="sequence" eval="20"/>
            <field name="view" ref="contingency_keys_view_form"/>
            <field name="act_window" ref="act_contingency_keys_tree"/>
        </record>
        <menuitem parent="account_invoice.menu_invoices" action="act_contingency_keys_tree"
            id="menu_contingency_keys_tree"/>
    </data>
</tryton>
